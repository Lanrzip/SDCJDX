QWidget#DownWidget{	background-color: #3a3a3a;	color: #fff;	selection-background-color: #b78620;	selection-color: #000;}QLabel#Label{	color: #b9b9bb;	border-color: #000000;}QPlainTextEdit{	background-color: #4d4d4d;	color : #eee;	border: 1px solid #343434;	border-radius: 2px;	padding: 3px;	padding-left: 5px;}QPushButton#Button{	background-color: #ff9c2b;	color: #000000;	font-weight: bold;	border-style: solid;	border-color: #000000;	padding: 6px;}QPushButton::hover#Button{	background-color: rgba(183, 134, 32, 20%);	border: 1px solid #b78620;}QPushButton::pressed#Button{	background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgba(74, 74, 74, 255),stop:1 rgba(49, 49, 49, 255));	border: 1px solid #b78620;}QCheckBox{	background-color: transparent;    color: lightgray;	border: none;}QCheckBox::indicator{    background-color: #323232;    border: 1px solid darkgray;    width: 12px;    height: 12px;}QCheckBox::indicator:checked{	background-color: #b78620;    border: 1px solid #3a546e;}QCheckBox::indicator:unchecked:hover{	border: 1px solid #b78620;}QRadioButton{	color: #b9b9bb;	border-color: #000000;}QRadioButton::indicator::unchecked:hover{	background-color: #d3d3d3;	border: 2px solid #002b2b;	border-radius: 6px;}QRadioButton::indicator::checked{	border: 2px solid #52beff;	border-radius: 6px;	background-color: #002b2b;	width: 9px;	height: 9px;}QComboBox{    background-color: #4d4d4d;    padding-left: 6px;    color: #fff;    height: 20px;	border-radius: 4px;	border: 1px solid #000;}QComboBox::disabled{	background-color: #47484c;	color: #b9b9bb;	border-color: #051a39;}QComboBox:on{    background-color: #a7a8bd;	color: #fff;}QComboBox QAbstractItemView{    background-color: #3a3a3a;    color: #b9b9bb;    selection-background-color: #002b2b;	selection-color: #fff;    outline: 0;}QComboBox::drop-down{	background-color: #5e616d;    subcontrol-origin: padding;    subcontrol-position: top right;	border-radius: 4px;    width: 15px;}QComboBox::down-arrow{    image: url(res/arrow_down.png);    width: 8px;    height: 8px;}QLineEdit#Edit{	background-color: #4d4d4d;	color: #fff;	font-weight: bold;	border-style: solid;	border-radius: 5px;	padding: 5px;}QLineEdit#rEdit{	background-color: #4d4d4d;	color: #fff;	font-weight: bold;	border-style: solid;	border-radius: 5px;	padding: 5px;}QLineEdit::disabled#rEdit{	background-color: #4d4d4d;	color: #b9b9bb;	font-weight: bold;	border-style: solid;	border-radius: 5px;	padding: 5px;}